{
    "rootDir": "../../",
    "roots": ["<rootDir>/lambda/custom"],
    "verbose": true,
    "testPathIgnorePatterns": [
        "/.backup/",
        "./lambda/custom/resources/"
        ],
    "coveragePathIgnorePatterns": [
        "./lambda/custom/index.js",
        "./lambda/custom/handlers/core/index.js",
        "./lambda/custom/handlers/custom/index.js",
        "./lambda/custom/resources/",
        "/node_modules/"
    ],
    "collectCoverageFrom": ["lambda/custom/**/*.js"],
    "testEnvironment": "node",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
}